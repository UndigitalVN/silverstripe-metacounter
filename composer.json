{
	"name": "undigitalvn/silverstripe-metacounter",
	"description": "Adds a counter to the meta fields for remaining characters",
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "metadata", "counter", "limit"],
	"authors": [{
		"name": "Undigital VN",
		"email": "support@undigital.com.au"
	}],
	"require": {
		"silverstripe/cms": "^6"
	},
	"suggest": {
		"kinglozzer/metatitle": "Adds meta-title field"
	},
	"replace": {
		"xini/silverstripe-metacounter": "self.version"
	},
	"autoload": {
		"psr-4": {
			"Innoweb\\MetaCounter\\": "src/"
		}
	},
	"extra": {
		"expose": [
			"client/dist"
		]
	}
}
